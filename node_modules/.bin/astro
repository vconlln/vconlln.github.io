#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/vconlln/myblog/vconlln.github/vconlln.github.io/node_modules/.pnpm/astro@4.15.0_@types+node@22.5.1_lightningcss@1.26.0_rollup@2.79.1_sass@1.77.8_stylus@0.63.0_t_wgmmsypokaff6r5suhxr4lea7i/node_modules/astro/node_modules:/home/vconlln/myblog/vconlln.github/vconlln.github.io/node_modules/.pnpm/astro@4.15.0_@types+node@22.5.1_lightningcss@1.26.0_rollup@2.79.1_sass@1.77.8_stylus@0.63.0_t_wgmmsypokaff6r5suhxr4lea7i/node_modules:/home/vconlln/myblog/vconlln.github/vconlln.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/vconlln/myblog/vconlln.github/vconlln.github.io/node_modules/.pnpm/astro@4.15.0_@types+node@22.5.1_lightningcss@1.26.0_rollup@2.79.1_sass@1.77.8_stylus@0.63.0_t_wgmmsypokaff6r5suhxr4lea7i/node_modules/astro/node_modules:/home/vconlln/myblog/vconlln.github/vconlln.github.io/node_modules/.pnpm/astro@4.15.0_@types+node@22.5.1_lightningcss@1.26.0_rollup@2.79.1_sass@1.77.8_stylus@0.63.0_t_wgmmsypokaff6r5suhxr4lea7i/node_modules:/home/vconlln/myblog/vconlln.github/vconlln.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
